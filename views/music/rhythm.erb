\version "2.24.1"

#(set-global-staff-size 25)

\new StaffGroup
<<
    <% @staves.each do |notes| %>
        \new RhythmicStaff {
            \time <%= @time %>

            \override Score.BarLine.stencil = ##f
            \override Score.TimeSignature.stencil = ##f

            <%= notes %>
        }
    <% end %>
>>